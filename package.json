{
  "name": "bsale-sdk",
  "version": "0.1.0",
  "description": "Complete TypeScript SDK for Bsale Chile API v1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "bsale",
    "chile",
    "api",
    "sdk",
    "typescript",
    "e-commerce",
    "inventory"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^2.1.8",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  }
}